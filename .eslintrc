{
    "extends": ["react-app", "airbnb", "prettier", "prettier/react"],
    "plugins": ["prettier"],
    "rules": {
      "no-use-before-define": 0,
      "react/jsx-props-no-spreading": 0,
      "react/prop-types": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error",
        {
          "no-use-before-define": false
        }
      ]
    }
  }
  